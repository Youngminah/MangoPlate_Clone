# platform :ios, '9.0'

target 'MangoPlate_Clone' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MangoPlate_Clone

  pod 'Alamofire'
  pod 'AlamofireNetworkActivityIndicator'
  pod 'SnapKit'
  pod 'KakaoSDKCommon'  # 필수 요소를 담은 공통 모듈
